{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",

    "esModuleInterop": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,

    "strict": false,
    "noImplicitAny": false,

    "skipLibCheck": true,
    "types": ["react", "react-dom", "node"]
  },
  "include": ["src"]
}
